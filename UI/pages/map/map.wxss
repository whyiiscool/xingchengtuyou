.tui-map-search{
  width: 100%;
  height: 80rpx;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000000;
  box-sizing: border-box;
  padding: 5rpx 10px;
  background-color: #fff;
  line-height: 70rpx;
}
.tui-map-input{
  height: 70rpx;
  line-height: 70rpx;
  font-size: 30rpx;
  margin-left: 25px;
}
.tui-map-search-icon{
  position: absolute;
  top: calc(50% - 10px);
  left: 10px;
}
.tui-map{
  width: 100%;
  height: calc(100% - 100rpx);
  position: fixed;
  bottom: 0;
  left: 0;
}
.tui-map-direction{
  width: 32px;
  height: 60px;
  position: fixed;
  right: 10px;
  bottom: 80px;
  z-index: 100000;
}

.page-group{
  display: table;
  width: 100%;
  table-layout: fixed;
  background-color: #fff;
}
.page-nav-list{
  padding:19rpx 0 ;
  font-size: 30rpx;
  display: table-cell;
  text-align: center;
  width: 30%;
  color: #222; 
  flex-direction:row;
  justify-content:center;
}
.page-nav-list.active{color:steelblue;}
.tui-warn{
  height: 48px;
  line-height: 50px;
  padding-left: 10px;
  color: lightseagreen;
  font-size: 29rpx;
}
.tui-search-bottom{
  height: 200px;
  background: #fff;
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 1000;
}
.jiache{
  width: 45px;
  height:15px;
}
button::after{
  border:none;
  
}
.hz{
   color:steelblue;
   font-size: 15px;
}